<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BdsView">
        <attr name="isSelected" format="boolean" />
        <attr name="isEnabled" format="boolean" />
        <attr name="bds_textAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="BdsFilter">
        <attr name="isSelected" />
        <attr name="isEnabled" />
        <attr name="bds_textAppearance" />
        <attr name="bds_text" format="string" />
        <attr name="bds_startDrawable" format="reference" />
        <attr name="bds_endDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="BdsButton">
        <attr name="isEnabled" />
        <attr name="bds_text" />
        <attr name="bds_startDrawable" />
        <attr name="bds_endDrawable" />
    </declare-styleable>
</resources>
